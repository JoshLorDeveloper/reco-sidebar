{
  "manifest_version": 2,

  "name": "Reco",
  "description": "The internet's comment section.",
  "version": "1.0",
  "browser_action": {
  },
  "background": {
      "scripts":["config/config.js", "scripts/background.js"]
  },
  "content_scripts": [{
      "js": ["js/contentscript.js", "js/jquery.min.js"],
      "matches": ["*://*/*"],
      "all_frames": false
  }],
  "web_accessible_resources": [
      "views/frame.html"
  ],
  "permissions": [
      "storage",
      "tabs",
      "identity",
      "http://*/",
      "https://*/"
  ]
}
